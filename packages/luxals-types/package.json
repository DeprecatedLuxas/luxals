{
  "name": "@luxass/luxals-types",
  "version": "2.0.0",
  "description": "A collection of TypeScript types",
  "author": "Lucas Norgaard <lucasnrgaard@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "ts",
    "types",
    "utility",
    "util",
    "utilities"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  }
}
