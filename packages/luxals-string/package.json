{
  "name": "@luxass/luxals-string",
  "version": "2.0.0",
  "packageManager": "pnpm@7.1.7",
  "description": "A collection of JavaScript & TypeScript utilities.",
  "author": "Lucas Norgaard <lucasnrgaard@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vitest": "^0.13.1"
  }
}
